<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="50.0" stylesheets="@../styles/main.css" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.102-ea" fx:controller="com.rps.controller.AboutMenuController">
    <Label text="About" GridPane.halignment="CENTER"/>
    <Label fx:id="descriptionLabel" text="Rps is a Rock-Paper-Scissors game made with javaFX" GridPane.rowIndex="1"/>
    <Button mnemonicParsing="false" onAction="#handleAboutBackBtn" text="Go Back" GridPane.halignment="CENTER"
            GridPane.rowIndex="2">
        <GridPane.margin>
            <Insets top="10.0"/>
        </GridPane.margin>
    </Button>
</GridPane>