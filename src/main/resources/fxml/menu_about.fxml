<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="50.0" stylesheets="@../styles/main.css"
          vgap="10" xmlns="http://javafx.com/javafx/8.0.102-ea"
          fx:controller="com.rps.controller.AboutMenuController">
    <Label text="About" GridPane.halignment="CENTER"/>
    <Label fx:id="descriptionLabel" text="Rps is a Rock-Paper-Scissors game made with javaFX." GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets top="15.0"/>
        </GridPane.margin>
    </Label>
    <Label fx:id="contributorsLabel" text="Contributors" GridPane.rowIndex="2" GridPane.halignment="CENTER">
        <GridPane.margin>
            <Insets top="15.0"/>
        </GridPane.margin>
    </Label>
    <Label fx:id="contributor1Label" text="Myeongjun Kim" GridPane.rowIndex="3" GridPane.halignment="CENTER">
    </Label>
    <Label fx:id="contributor2Label" text="Hyunjae Lee" GridPane.rowIndex="4" GridPane.halignment="CENTER">
        <GridPane.margin>
            <Insets bottom="100.0"/>
        </GridPane.margin>
    </Label>
    <Button mnemonicParsing="false" onAction="#handleAboutBackBtn" text="Go Back" GridPane.halignment="CENTER"
            GridPane.rowIndex="5"/>
</GridPane>